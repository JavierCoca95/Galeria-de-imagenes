import java.util.Scanner;
public class Paper
{
  public static int paperWork(int n, int m){
    int result = n*m;
     if (n <= 0) {
            System.out.println("0");
       return 0;
        } else if (m <= 0) {
            System.out.println("0");
       return 0;
        } else System.out.println(result);
        return result;
    }
  }
